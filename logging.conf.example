[loggers]
keys=root,mcp.server,mcp_talib,uvicorn,uvicorn.access,uvicorn.error,uvicorn.server,fastapi,mcp.server.fastmcp

[handlers]
keys=consoleFile

[formatters]
keys=standard

[logger_root]
level=INFO
handlers=consoleFile

[logger_mcp.server]
level=INFO
handlers=consoleFile
qualname=mcp.server
propagate=0

[logger_mcp_talib]
level=INFO
handlers=consoleFile
qualname=mcp_talib
propagate=0

[logger_uvicorn]
level=INFO
handlers=consoleFile
qualname=uvicorn
propagate=0

[logger_uvicorn.access]
level=INFO
handlers=consoleFile
qualname=uvicorn.access
propagate=0

[logger_uvicorn.error]
level=INFO
handlers=consoleFile
qualname=uvicorn.error
propagate=0

[logger_uvicorn.server]
level=INFO
handlers=consoleFile
qualname=uvicorn.server
propagate=0

[logger_fastapi]
level=INFO
handlers=consoleFile
qualname=fastapi
propagate=0

[logger_mcp.server.fastmcp]
level=INFO
handlers=consoleFile
qualname=mcp.server.fastmcp
propagate=0

[handler_consoleFile]
class=FileHandler
level=DEBUG
formatter=standard
args=("console.log", "a", "utf-8")

[formatter_standard]
format=%(asctime)s - %(name)s - %(levelname)s - %(message)s
